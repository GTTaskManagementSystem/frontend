{
  "name": "frontend",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && npx simple-git-hooks",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --fix --cache .",
    "generate-pwa-icons": "tsx scripts/generate-pwa-icons.ts",
    "reset": "rm -rf .nuxt dist yarn.lock node_modules && yarn install",
    "cleanup": "rm -rf .nuxt dist"
  },
  "dependencies": {
    "@iconify-json/fa6-solid": "^1.1.15",
    "nuxt-security": "^0.14.4",
    "simple-git": "^3.18.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@nuxt/devtools": "latest",
    "@una-ui/nuxt": "npm:@una-ui/nuxt-edge@latest",
    "@unocss/eslint-config": "^0.51.12",
    "eslint": "^8.49.0",
    "lint-staged": "^14.0.1",
    "nuxt": "npm:nuxt3@latest",
    "sharp": "^0.32.5",
    "sharp-ico": "^0.1.5",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.2.2",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.8.10"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
